{
  "name": "angular2webpack-seed",
  "version": "1.0.0",
  "description": "angular2 seed with webpack",
  "main": "index.js",
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  },
  "scripts": {
    "validate": "npm-run-all --parallel validate-webpack:* lint test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "start": "webpack-dev-server  --hot  --history-api-fallback --env.dev",
    "serve:prod": "webpack-dev-server  --hot  --history-api-fallback --env.prod",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy \"**/index.html\" favicon.ico \"../dist\" --cwd=src --parents",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "build:prod": "webpack --env.prod",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "postinstall": "typings install",
    "coverage": "http-server -c-1 -o -p 9875 ./coverage"
  },
  "author": "js",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "^3.0.0",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.1.1",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "bootstrap-css-only": "^3.3.6",
    "browser-sync": "^2.13.0",
    "codelyzer": "^0.0.26",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "ghooks": "^1.3.2",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "~4.1.1",
    "npm-run-all": "^2.3.0",
    "postcss-loader": "^1.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "sass-loader": "^6.0.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "typescript": "2.2.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dashboard": "^0.0.1",
    "webpack-dev-server": "2.3.0",
    "webpack-merge": "0.14.1",
    "webpack-node-externals": "1.5.4",
    "webpack-validator": "2.3.0"
  }
}
